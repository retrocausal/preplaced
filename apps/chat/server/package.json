{
  "name": "server",
  "type": "module",
  "version": "1.0.0",
  "entry": "dist/app.js",
  "scripts": {
    "start:1": "PORT=4001 nodemon dist/app.js",
    "start:2": "PORT=4002 nodemon dist/app.js",
    "start:3": "PORT=4003 nodemon dist/app.js",
    "start:4": "PORT=4004 nodemon dist/app.js",
    "start:all": "concurrently \"npm run start:1\" \"npm run start:2\" \"npm run start:3\" \"npm run start:4\"",
    "build": "rm -rf dist && tsc",
    "watch": "tsc --watch",
    "dev": "npm run watch & npm run start:all",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "lint": "eslint . --ext .ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "8.17.2",
    "multer": "^2.0.2",
    "socket.io": "4.8.1",
    "ws": "^8.18.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "~5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.12.7",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^6.0.3",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.0.1",
    "response-time": "^2.3.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.5.2"
  },
  "imports": {
    "#*": "./dist/*.js",
    "#db/*": "./dist/db/*.js",
    "#models/*": "./dist/db/models/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#middlewares/*": "./dist/middlewares/*.js"
  }
}
